package problema1065;

import java.util.Scanner;

public class Problema1065 {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        int cont = 0;

        for (int i = 0; i < 5; i++) {
            double n = sc.nextDouble();
            if (n % 2 == 0) {
                cont++;
            }
        }
        System.out.println(cont + " valores pares");
    }

}
